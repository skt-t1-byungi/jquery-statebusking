      "require": true,
      "module": true
    }
  },
  "eslintIgnore": [
    "jquery-statebusking.min.js",
    "test/*"
  ],
  "devDependencies": {
    "ava": "^0.25.0",
    "browser-env": "^3.2.5",
    "eslint": "^4.19.1",
    "eslint-config-byungi": "^0.0.16",
    "jquery": "^3.3.1",
    "jquery-statebus": "^0.4.4",
    "uglify-js": "^3.4.2"
  },
  "ava": {
    "require": [
      "./test/helpers/env.js"
    ]
