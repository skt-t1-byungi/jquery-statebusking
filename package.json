{
  "name": "jquery-statetbusking",
  "description": "Backbone alternative using jquery-statebus",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "skt-t1-byungi",
    "email": "tiniwz@gmail.com"
  },
  "keywords": [
    "jquery",
    "mvc",
    "backbone",
    "statebus",
    "state"
  ],
  "main": "jquery-statebusking.js",
  "unpkg": "jquery-statebusking.min.js",
  "files": [
    "jquery-statebusking.js",
